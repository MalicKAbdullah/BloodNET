@{
    var isAdmin = Context.Session.GetString("IsAdmin");
    if (!string.IsNullOrEmpty(isAdmin) && isAdmin == "true")
    {
        Layout = "_LayoutAdmin";
    }
    else
    {
        if (User.Identity.IsAuthenticated)
        {
            Layout = "_LayoutUser";
        }
        else
        {
            Layout = "_Layout";
        }
    }

    
}
